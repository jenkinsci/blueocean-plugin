{
  "name": "blueocean-web",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jenkinsci/blueocean-plugin.git"
  },
  "scripts": {
    "lint": "gulp lint",
    "lint:fix": "gulp lint --fixLint",
    "test": "gulp test",
    "test:watch": "gulp test:watch",
    "bundle": "gulp bundle",
    "bundle:watch": "gulp bundle:watch",
    "mvnbuild": "gulp lint bundle && browserify src/main/js/ie/iepolyfills.js > target/classes/org/jenkins/ui/jsmodules/blueocean-web/iepolyfills.js",
    "mvnbuild:fast": "gulp bundle",
    "mvntest": "gulp test"
  },
  "devDependencies": {
    "@jenkins-cd/js-builder": "0.0.64",
    "babel-eslint": "7.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "bluebird-global": "1.0.1",
    "create-hmac": "1.1.4",
    "eslint": "2.13.1",
    "eslint-plugin-react": "4.3.0",
    "giti": "1.1.3",
    "gulp": "3.9.1",
    "gulp-typescript": "4.0.1",
    "jest": "29.6.1",
    "ncp": "2.0.0",
    "reactify": "1.1.1",
    "ts-jest": "29.1.1",
    "tsify": "4.0.0",
    "typescript": "2.7.2"
  },
  "dependencies": {
    "@jenkins-cd/blueocean-core-js": "1.10.4",
    "@jenkins-cd/design-language": "1.10.4",
    "@jenkins-cd/js-extensions": "0.0.44",
    "@jenkins-cd/js-modules": "0.0.10",
    "history": "2.0.2",
    "immutable": "3.8.1",
    "keymirror": "0.1.1",
    "mobx": "2.6.0",
    "mobx-react": "3.5.7",
    "mobx-react-devtools": "4.2.6",
    "moment": "2.19.3",
    "react": "15.4.2",
    "react-addons-css-transition-group": "15.4.2",
    "react-dom": "15.4.2",
    "react-i18next": "1.10.1",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0"
  },
  "jenkinscd": {
    "import": [
      "@jenkins-cd/blueocean-core-js@any",
      "@jenkins-cd/design-language@any",
      "@jenkins-cd/js-extensions@any",
      "moment@any",
      "react@any",
      "react-addons-css-transition-group@any",
      "react-dom@any",
      "react-router@any"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "browserify": {
    "extensions": [
      ".js",
      ".es6",
      ".jsx",
      ".hbs",
      ".ts",
      ".tsx"
    ],
    "plugin": [
      [
        "tsify"
      ]
    ]
  }
}
