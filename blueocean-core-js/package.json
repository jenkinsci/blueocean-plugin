    "i18next-browser-languagedetector": "1.0.0",
    "i18next-xhr-backend": "1.2.0",
    "isomorphic-fetch": "2.2.1",
    "jsonwebtoken": "9.0.0",
    "mobx": "2.6.0",
    "mobx-utils": "1.1.2",
    "moment": "2.19.3",
    "moment-duration-format": "2.2.2",
    "pem-jwk": "1.5.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router": "2.8.1"
  },
  "devDependencies": {
    "@jenkins-cd/eslint-config-jenkins": "0.0.2",
    "@jenkins-cd/js-builder": "0.0.64",
    "@jenkins-cd/js-builder-jest": "0.0.4",
    "@kadira/storybook": "2.20.1",
    "@types/bluebird": "3.5.0",
    "@types/bluebird-global": "3.5.0",
    "@types/node": "9.6.4",
    "babel-eslint": "7.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "browserify": "13.1.0",
    "chai": "3.5.0",
    "del": "2.2.2",
    "enzyme": "2.4.1",
    "eslint": "2.13.1",
    "eslint-plugin-react": "4.3.0",
    "flow-bin": "0.34.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-copy": "0.0.2",
    "gulp-eslint": "3.0.1",
    "gulp-less": "3.1.0",
    "gulp-rename": "1.2.2",
    "gulp-sourcemaps": "1.6.0",
    "gulp-typescript": "4.0.1",
    "jest": "19.0.2",
