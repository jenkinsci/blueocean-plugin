    "typescript": "2.7.2",
    "watchify": "3.7.0"
  },
  "jenkinscd": {
    "export": [
      "@jenkins-cd/js-extensions",
      "@jenkins-cd/logging",
      "mobx"
    ],
    "import": [
      "@jenkins-cd/design-language@any",
      "moment@any",
      "moment-duration-format@any",
      "react@any",
      "react-dom@any",
      "react-router@any"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
